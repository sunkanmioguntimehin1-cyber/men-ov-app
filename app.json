{
  "expo": {
    "name": "Menovia",
    "slug": "menovia",
    "owner": "menovia",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "menoviaapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "userInterfaceStyle": "light",
        "NSCameraUsageDescription": "This app uses the camera to allow users upload photos.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for video recording .",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can upload photos.",
        "FirebaseAutomaticScreenReportingEnabled": false
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.menovia.menovia"
    },
    "android": {
      "userInterfaceStyle": "light",
      "googleServicesFile": "./google-services2.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.menovia.menovia",
      "splash": {
        "dark": {
          "image": "./assets/images/splash-icon.png",
          "backgroundColor": "#171717"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#171717",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#171717"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.759542327992-971ok5geank09ddh8kaeoglsk9mqg1fo"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "podfileProperties": {
              "use_modular_headers!": true
            },
            "buildReactNativeFromSource": true
          }
        }
      ],
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "99d071a9-b0d9-4db0-85b7-90a308101011"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/99d071a9-b0d9-4db0-85b7-90a308101011"
    }
  }
}
